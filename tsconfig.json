{
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "ES2015",
    "noImplicitAny": true,
    "target": "ES5",
    "strict": true,
    "moduleResolution": "node",
    "outDir": "docs}",
    "sourceMap": true
  },
  "include": [
    "src"
  ]
}